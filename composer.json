{
	"name": "doubleedesign/acf-dynamic-preview",
	"version": "0.0.1",
	"description": "Dynamic render and preview functionality for ACF flexible content modules, forked from the ACF Extended plugin.",
	"type": "wordpress-plugin",
	"authors": [
		{
			"name": "Leesa Ward",
			"email": "leesa@doubleedesign.com.au"
		}
	],
	"autoload": {
		"psr-4": {
			"Doubleedesign\\ACFDynamicPreview\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"laravel/pint": "^1.24",
		"pestphp/pest": "^3.8"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
